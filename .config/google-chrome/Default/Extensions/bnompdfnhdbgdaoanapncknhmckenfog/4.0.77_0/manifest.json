{
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": "image/icon_inactive.38.png",
      "default_popup": "popup.html",
      "default_title": "Email Tracker"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "css/content.css" ],
      "js": [ "lib/global.js", "lib/xtion.js", "lib/moment/moment-with-locales.min.js", "lib/moment/moment-timezone-with-data.min.js", "lib/gmailsdk.js", "content.js" ],
      "matches": [ "*://*.mail.google.com/*", "*://*.inbox.google.com/*", "*://*.mail.live.com/*", "*://*.outlook.live.com/*", "*://*.outlook.office.com/*", "*://*.outlook.office365.com/*", "*://*.mail.yahoo.com/*", "*://*/owa/*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.gstatic.com https://www.google-analytics.com/analytics.js; object-src 'self'",
   "default_locale": "en",
